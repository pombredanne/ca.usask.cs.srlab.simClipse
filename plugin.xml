               id="ca.usask.cs.srlab.simclipse.actions.openSimclipseNavigatorView"
               label="%action.navigatorView.label"
               menubarPath="ca.usask.cs.srlab.simclipse.workbenchMenu/content"
               style="push"
               toolbarPath="Normal/additions">
         </action>
      </actionSet>
