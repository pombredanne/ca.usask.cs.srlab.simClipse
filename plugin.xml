                  icon="icons/enable.png"
                  id="ca.usask.cs.srlab.simclipse.popups.enableSimClipse"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <iterate
                           ifEmpty="false"
                           operator="or">
                        <and>
                        	<or>
	                           	<instanceof 
	                           		value="org.eclipse.core.resources.IProject">
	                           	</instanceof>
	                           <instanceof
	                                value="org.eclipse.jdt.core.IJavaProject">
	                           </instanceof>
                           	</or>
                           <test
                                 forcePluginActivation="true"
                                 property="ca.usask.cs.srlab.simclipse.notSimClipseItem">
                           </test>
                           
